      <JFXButton fx:id="addChildButton" onAction="#onAddChild" prefHeight="62.0" prefWidth="564.0" stylesheets="@button.css" text="اضافة طفل" />
      <JFXButton fx:id="addSessionButton" layoutX="10.0" layoutY="10.0" onAction="#onAddSession" prefHeight="62.0" prefWidth="622.0" stylesheets="@button.css" text="اضافة جلسة" />
