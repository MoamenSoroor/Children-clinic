<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="CENTER" prefHeight="524.0" prefWidth="345.0" spacing="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.60" fx:controller="application.sub.DelStatusController">
   <children>
      <Label text="حذف تشخيص أو أكثر">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      <Separator prefWidth="200.0" />
      <ListView fx:id="statusList" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
      <HBox alignment="CENTER" prefHeight="56.0" prefWidth="303.0" spacing="20.0">
         <children>
            <Button fx:id="deleteStatus" mnemonicParsing="false" onAction="#onDeleteStatus" prefHeight="28.0" prefWidth="99.0" text="حذف" />
            <Button fx:id="cancel" layoutX="112.0" layoutY="24.0" mnemonicParsing="false" onAction="#onCancel" prefHeight="28.0" prefWidth="99.0" text="رجوع" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
